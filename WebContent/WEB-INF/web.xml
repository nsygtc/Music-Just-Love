<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>ManageSystem/index.jsp</welcome-file>
  </welcome-file-list>
  
  
  
  
  <servlet>
  	<servlet-name>music</servlet-name>
  	<servlet-class>cn.itcast.servlet.MusicServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>User</servlet-name>
    <servlet-class>cn.itcast.servlet.UserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Video</servlet-name>
    <servlet-class>cn.itcast.servlet.VideoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Theory</servlet-name>
    <servlet-class>cn.itcast.servlet.TheoryServlet</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
  	<servlet-name>User</servlet-name>
  	<url-pattern>/ManageSystem/User</url-pattern>
  </servlet-mapping>  
  <servlet-mapping>
    <servlet-name>music</servlet-name>
    <url-pattern>/ManageSystem/music</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Video</servlet-name>
    <url-pattern>/ManageSystem/Video</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Theory</servlet-name>
    <url-pattern>/ManageSystem/Theory</url-pattern>
  </servlet-mapping>
  
  
</web-app>